  "scripts": {
    "clean": "rimraf ./build && rimraf ./coverage",
    "clean:build": "rimraf ./build",
    "clean:coverage": "rimraf ./coverage",
    "build": "cmf-settings && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --progress",
    "build:dist": "cmf-settings && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --progress",
    "prebuild": "rimraf ./build",
    "start": "cmf-settings && cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js --progress --content-base build/",
    "test": "cross-env NODE_ENV=test karma start && jest",
    "test:karma": "cross-env NODE_ENV=test karma start",
    "test:jest": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test karma start karma.conf.ci.js && jest",
    "test:auto": "cross-env NODE_ENV=test karma start --auto",
    "lint:es": "eslint --config .eslintrc --fix src",
    "lint:sass": "sass-lint -c .sass-lint.yml 'src/app/**/*.scss' -v -q",
    "stats": "webpack --config config/webpack.config.dev.js --profile -j > stats.json"
  },
  "jest": {
    "testRegex": "/src/.*\\.test.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/src/test/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
