  "props": {
    "AppLoader#default": {
      "saga": "bootstrap",
      "hasCollections": [
        "settings"
      ]
    },
    "Breadcrumbs#default": {},
    "HeaderBar#default": {
      "app": "Data Preparation",
      "logo": {
        "id": "talend-logo",
        "label": {
          "i18n": {
            "key": "tdp-cmf:GOTO_HOME",
            "options": {
              "defaultValue": "Go to home page"
            }
          }
        },
        "isFull": false,
        "payload": {
          "type": "MENU_LINK",
          "cmf": {
            "routerReplace": "/preparations"
          }
        }
      },
      "brand": {
        "label": "Data Preparation  (react-cmf)",
        "payload": {
          "type": "MENU_LINK",
          "cmf": {
            "routerReplace": "/preparations"
          }
        }
      },
      "search": {
        "id": "headerbar:search",
        "componentId": "headerbar:search",
        "icon": {
          "name": "talend-search",
          "title": {
            "i18n": {
              "key": "tdp-cmf:SEARCH_TOGGLE",
              "options": {
                "defaultValue": "Toggle search input"
              }
            }
          },
          "bsStyle": "link",
          "tooltipPlacement": "bottom"
        },
        "onChangeActionCreator": "headerbar:search:start",
        "onSelectActionCreator": "headerbar:search:select",
        "onBlurActionCreator": "headerbar:search:reset",
        "itemsExpression": {
          "id": "cmf.collections.get",
          "args": [
            "search",
            null
          ]
        }
      },
      "help": {
        "id": "header-help",
        "icon": "talend-question-circle",
        "label": {
          "i18n": {
            "key": "tdp-cmf:HELP",
            "options": {
              "defaultValue": "Help"
            }
          }
        },
        "target": "_blank",
        "href": "https://help.talend.com/search/all?filters=EnrichPlatform~%2522Talend+Data+Preparation%2522*EnrichVersion~%2522Cloud%2522&utm_medium=dpdesktop&utm_source=header"
      },
      "information": {
        "id": "headerbar:information",
        "bsStyle": "link",
        "icon": "talend-info-circle",
        "label": {
          "i18n": {
            "key": "tdp-cmf:INFORMATION",
            "options": {
              "defaultValue": "Information"
            }
          }
        },
        "hideLabel": true,
        "pullRight": true,
        "noCaret": true,
        "tooltipPlacement": "bottom",
        "displayMode": "dropdown",
        "actionIds": [
          "help:about:open",
          "help:tour",
          "divider",
          "help:community",
          "help:feedback:open"
        ]
      }
    },
    "SidePanel#default": {
      "actionIds": [
        "menu:preparations",
        "menu:datasets",
        "menu:datastores"
      ]
    },
    "Version#default": {
      "displayVersionExpression": {
        "id": "cmf.collections.get",
        "args": [
          "versions.displayVersion",
          []
        ]
      },
      "servicesExpression": {
        "id": "cmf.collections.get",
        "args": [
          "versions.services",
          []
        ]
      }
    },
    "AboutModal#default": {
      "components": {
        "content": []
      }
    },
    "HomeListView#datastores": {
      "hasTheme": true
    },
    "HomeListView#datasets": {
      "hasTheme": true
    },
    "HomeListView#preparations": {
      "didMountActionCreator": "preparation:fetch",
      "hasTheme": true,
      "header": {},
      "sidepanel": {},
      "list": {
        "components": {
          "before-component": [
            {
              "component": "Breadcrumbs"
            }
          ]
        },
        "collectionId": "preparations",
        "list": {
          "columns": [
            {
              "key": "name",
              "label": {
                "i18n": {
                  "key": "tdp-cmf:NAME",
                  "options": {
                    "defaultValue": "Name"
                  }
                }
              }
            },
            {
              "key": "author",
              "label": {
                "i18n": {
                  "key": "tdp-cmf:AUTHOR",
                  "options": {
                    "defaultValue": "Author"
                  }
                }
              }
            },
            {
              "key": "datasetName",
              "label": {
                "i18n": {
                  "key": "tdp-cmf:DATASET",
                  "options": {
                    "defaultValue": "Dataset"
                  }
                }
              }
            },
            {
              "key": "nbSteps",
              "label": {
                "i18n": {
                  "key": "tdp-cmf:STEPS",
                  "options": {
                    "defaultValue": "Steps"
                  }
                }
              }
            }
          ],
          "titleProps": {
            "key": "name",
            "iconKey": "icon",
            "displayModeKey": "display"
          }
        },
        "toolbar": {
          "filter": {
            "placeholder": {
              "i18n": {
                "key": "tdp-cmf:PREPARATION_FIND",
                "options": {
                  "defaultValue": "Find a preparation"
                }
              }
            }
          }
        },
        "actions": {
          "title": "preparation:open",
          "left": [
            "preparation:add:open",
            "folder:add:open"
          ],
          "items": [
            "preparation:rename",
            "preparation:duplicate"
          ],
          "editSubmit": "preparation:edit:submit",
          "editCancel": "preparation:edit:cancel"
        }
      }
    }
  },
  "ref": {
    "SidePanel#default": {},
    "AppHeaderBar#default": {}
  }
}
